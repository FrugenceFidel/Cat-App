<% include ./partials/header %>

<div class="container">
	<h2><%= cat.name%></h2>
	<img src="<%= cat.image %>">
	<p><%= cat.decription %></p>
	<span><%= cat.createdAt.toDateString() %></span>
	<p><a class="btn btn-default" role="button" href="/cats/<%= cat._id %>/edit">Edit</a></p>

	<form action="/cats/<%= cat._id %>?_method=DELETE" method="POST">
		<button class="btn btn-default">Delete</button>
	</form>
</div>

<% include ./partials/footer %>
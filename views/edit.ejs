<% include ./partials/header %>

<div class="container">
	<h2>Edit <%= cat.name %></h2>

	<form action="/cats/<%= cat._id %>?_method=PUT" method="POST">
		<input type="text" name="cat[name]" value="<%= cat.name %>">
		<input type="text" name="cat[image]" value="<%= cat.image %>">
		<input type="text" name="cat[decription]" value="<%= cat.decription %>">
		<button>Update</button>
	</form>
</div>

<% include ./partials/footer %>